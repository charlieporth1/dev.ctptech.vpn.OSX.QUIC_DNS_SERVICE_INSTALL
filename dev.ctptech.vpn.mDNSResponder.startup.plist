<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>dev.ctptech.vpn.mDNSResponder.startup</string> 
    
    <key>StandardOutPath</key>
    <string>/var/log/dev.ctptech.vpn.mDNSResponder.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/dev.ctptech.vpn.mDNSResponder.log</string>

 <key>ProgramArguments</key>
  <array>
    <string>/bin/launchctl</string>
    <string>setenv</string>
    <string>PATH</string>
    <string>/usr/loca/bin:/bin/usr/local/sbin:/sbin:/usr/bin:/usr/sbin:/var/root/go/bin</string>
  </array>
    <key>ProgramArguments</key>
    <array>
            <string>/bin/bash</string>
            <string>/usr/local/bin/dns-server</string>
    </array>

    <key>KeepAlive</key>
    <false/>
    <key>LaunchOnlyOnce</key>        
    <true/>
    <key>RunAtLoad</key>
    <true/>

</dict>
</plist>
